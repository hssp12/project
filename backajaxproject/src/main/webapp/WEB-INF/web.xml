  <servlet-mapping>
     <servlet-name>jsonList</servlet-name>
     <url-pattern>/list/list.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
     <servlet-name>jsonInsert</servlet-name>
     <url-pattern>/list/add.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
     <servlet-name>jsonOne</servlet-name>
     <url-pattern>/list/one.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>jsonUpdate</servlet-name>
  	<url-pattern>/list/update.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>jsonDelete</servlet-name>
  	<url-pattern>/list/delete.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>jsonList</servlet-name>
     <servlet-class>com.bit.ajax.JsonController</servlet-class>
  </servlet>
  <servlet>
     <servlet-name>jsonInsert</servlet-name>
     <servlet-class>com.bit.ajax.AddController</servlet-class>
  </servlet>
  <servlet>
     <servlet-name>jsonOne</servlet-name>
     <servlet-class>com.bit.ajax.OneController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>jsonUpdate</servlet-name>
  	<servlet-class>com.bit.ajax.UpdateController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>jsonDelete</servlet-name>
  	<servlet-class>com.bit.ajax.DeleteController</servlet-class>
  </servlet>

</web-app>
